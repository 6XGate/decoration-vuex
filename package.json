{
  "name": "decoration-vuex",
  "version": "2.1.1",
  "description": "Create type-safe class-based Vuex modules in TypeScript",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "dev": "npm run build:development",
    "prod": "npm run build:production",
    "coverage": "echo 'nyc ava'",
    "test": "echo 'ava'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run prod",
    "postpublish": "pinst --enable",
    "build": "npm run build:production",
    "build:development": "webpack --mode=development",
    "build:production": "webpack --mode=production --node-env=production"
  },
  "repository": "github:6XGate/decoration-vuex",
  "keywords": [
    "vue",
    "vuex",
    "typescript"
  ],
  "author": "Matthew Holder <sixxgate@hotmail.com> (https://www.sleepingcats.org/)",
  "license": "MIT",
  "bugs": "https://github.com/6XGate/decoration-vuex/issues",
  "homepage": "https://github.com/6XGate/decoration-vuex#readme",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "esm"
    ],
    "nodeArguments": [
      "--enable-source-maps"
    ]
  },
  "peerDependencies": {
    "vuex": "^3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "ava": "^3.15.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.22.0",
    "eslint-config-scalecore": "^1.0.10",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "esm": "^3.2.25",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-codemods": "^0.30.0",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.7.4",
    "vue": "^2.7.10",
    "vuex": "^3.6.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0",
    "webpackbar": "^5.0.2",
    "zod": "^3.18.0"
  }
}
