{
  "name": "decoration-vuex",
  "version": "2.1.1",
  "description": "Create type-safe class-based Vuex modules in TypeScript",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint src tests",
    "fix": "eslint --fix src tests",
    "dev": "echo 'rolete -c dev'",
    "prod": "echo 'rolete -c prod'",
    "coverage": "nyc ava",
    "test": "ava",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run prod",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6XGate/decoration-vuex.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "typescript"
  ],
  "author": "Matthew Holder (https://www.sleepingcats.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/6XGate/decoration-vuex/issues"
  },
  "homepage": "https://github.com/6XGate/decoration-vuex#readme",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "esm"
    ],
    "nodeArguments": [
      "--enable-source-maps"
    ]
  },
  "peerDependencies": {
    "vuex": "^3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/lodash": "^4.14.184",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "ava": "^3.15.0",
    "eslint": "^8.22.0",
    "eslint-config-scalecore": "^1.0.10",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "esm": "^3.2.25",
    "husky": "^8.0.1",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.7.4",
    "vue": "^2.7.10",
    "vuex": "^3.6.2"
  }
}
